; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Cave Maze"
config/tags=PackedStringArray("my_project")
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
config/icon="res://assets/textures/icons/icon.svg"

[audio]

general/text_to_speech=true

[autoload]

SceneDb="*res://global/databases/scene_db.gd"
maze="*res://global/managers/maze.gd"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
SaveSystem="*res://addons/save_system/SaveSystem.gd"
StateManager="*res://global/managers/state_manager.gd"
MouseManager="*res://global/mouse_cursor_manager.gd"
InteractionMng="*res://InteractionManager.gd"
GlobalEvents="*res://global/databases/global_events.gd"

[display]

window/size/viewport_width=480
window/size/viewport_height=270
window/size/initial_position_type=2
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
mouse_cursor/custom_image="res://assets/textures/icons/skel_hand_point.png"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogue_manager/plugin.cfg", "res://addons/save_system/plugin.cfg")

[global]

look_sensitivity=0.0

[gui]

fonts/dynamic_fonts/use_oversampling=false
theme/custom_font="res://assets/other/alagard_by_Hewett_Tsoi.ttf"
theme/default_font_subpixel_positioning=0

